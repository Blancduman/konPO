extends ./includes/layout

block content
  .row
    .mx-auto.col-md-7.col-lg-6.col-sm-12
      div(role="tablist").list-group
        each teacher in teachers
          a(href="/admin/teacher/"+teacher._id, role="tab").list-group-item.list-group-item-action.bg-dark
            h3.text-primary ФИО: #{teacher.lastname} #{teacher.firstname} #{teacher.middlename}
            h6.text-light Отключён: #{teacher.updatedAt}